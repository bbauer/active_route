%script{:type => "text/javascript", :src  => "https://www.google.com/jsapi"}
:javascript
  google.load("visualization", "1", {packages:["corechart"]});
  google.setOnLoadCallback(drawChart);
  function drawChart() {
    var data = new google.visualization.DataTable();
    data.addColumn('string', 'Grade');
    data.addColumn('number', 'Routes');
    data.addRows(7);
    data.setValue(0, 0, '5.6-');
    data.setValue(0, 1, 1);
    data.setValue(1, 0, '5.7');
    data.setValue(1, 1, 2);
    data.setValue(2, 0, '5.8');
    data.setValue(2, 1, 3);
    data.setValue(3, 0, '5.9');
    data.setValue(3, 1, 4);
    data.setValue(4, 0, '5.10');
    data.setValue(4, 1, 3);
    data.setValue(5, 0, '5.11');
    data.setValue(5, 1, 2);
    data.setValue(6, 0, '5.12+');
    data.setValue(6, 1, 1);
    var chart = new google.visualization.ColumnChart(document.getElementById('chart_div'));
    chart.draw(data, {width: 800, height: 540, title: 'Route Distribution', backgroundColor: '#eee',
                      hAxis: {title: 'Grade', titleTextStyle: {color: 'red'}}
                     });
  }
#chart_div
